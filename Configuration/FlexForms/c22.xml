<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>		
		<langDisable>1</langDisable>
	</meta>	
	<sheets>
		<eutinnews>
			<ROOT>
				<TCEforms>
					<sheetTitle>Newsfrontend Eutin</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.modus>
                        <TCEforms>
                            <label>Sortierung</label>
                            <config>
                                <type>select</type>
                                <size>1</size>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Datum</numIndex>
                                        <numIndex index="1">date</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Kategorien</numIndex>
                                        <numIndex index="1">kat</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>1</minitems>
                            </config>
                        </TCEforms>
                    </settings.modus>
                    <settings.categoriesList>
					   <TCEforms>
					   <exclude>1</exclude>
					   <label>Aktionen Kategorie ausw√§hlen</label>
					   <config>
					      <type>select</type>
					      <autoSizeMax>50</autoSizeMax>
					      <foreign_table>sys_category</foreign_table>
					      <foreign_table_where> AND sys_category.sys_language_uid IN (-1, 0) ORDER BY sys_category.sorting ASC</foreign_table_where>
					      <maxitems>9999</maxitems>
					      <renderMode>tree</renderMode>
					      <size>10</size>
					      <treeConfig>
					         <appearance>
					            <expandAll>1</expandAll>
					            <showHeader>1</showHeader>
					         </appearance>
					         <parentField>parent</parentField>
					      </treeConfig>
					   </config>
					   </TCEforms>
					</settings.categoriesList>
					<settings.categoriesConj>
                        <TCEforms>
                            <label>Kategorie Verbindung</label>
                            <config>
                                <type>select</type>
                                <size>1</size>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">OR</numIndex>
                                        <numIndex index="1">OR</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">AND</numIndex>
                                        <numIndex index="1">AND</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>1</minitems>
                            </config>
                        </TCEforms>
                    </settings.categoriesConj>
					<settings.anzahl>
						<TCEforms>
							<label>Anzahl</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</settings.anzahl>
					<settings.startview>
                        <TCEforms>
                            <label>Start Ansicht</label>
                            <config>
                                <type>select</type>
                                <size>1</size>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Zeitstrahl</numIndex>
                                        <numIndex index="1">time</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Liste</numIndex>
                                        <numIndex index="1">list</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>1</minitems>
                            </config>
                        </TCEforms>
                    </settings.startview>
                    <settings.mobilepreviewdis>                   
						<TCEforms>
							<label>Vorschau Liste Mobil ausschalten</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.mobilepreviewdis>
					<settings.viewsupport>                   
						<TCEforms>
							<label>Listen- und Zeitansicht Schalter</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.viewsupport>
					<settings.startingpoint>
					  <TCEforms>
						<exclude>1</exclude>
						<label>Datensatzsammlung</label>
						<config>
						  <type>group</type>
						<internal_type>db</internal_type>
							<allowed>pages</allowed>
						<size>3</size>
						<maxitems>50</maxitems>
						<minitems>0</minitems>
						<show_thumbs>1</show_thumbs>
						<wizards>
						  <suggest>
							<type>suggest</type>
						  </suggest>
						</wizards>
						</config>
					  </TCEforms>
					</settings.startingpoint>
				</el>
			</ROOT>
		</eutinnews>
	</sheets>
</T3DataStructure>
